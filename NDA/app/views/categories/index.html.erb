<h1> Disaster Categories</h1>

  <table>
      <tr>
        <th> Disasters </th>
      </tr>
      <td><%= link_to 'New Category', new_category_path(@categories) %> </td>

      <% @categories.each do |category| %>
        <tr>
          <td> <% category.title %></td>
          <img src = "<%= category.photo_url %>"/>
            <td><%= link_to 'Show', category_path(category) %></td>
          </tr>
          <% end %>
        </table>
